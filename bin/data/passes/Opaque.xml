<?xml version="1.0"?>
<pass>
    <filters>
        <filter source="Opaque/Deferred" />
        <filter source="Opaque/Ambient" />
        <filter source="Opaque/Lighting" />
        <filter source="Opaque/SSAO" />
        <filter source="Opaque/BlurSSAO">
            <defines>
                <define name="gs_nAxis" value="float2(1,0)" />
                <define name="gs_bCombine" value="0" />
            </defines>
        </filter>
        <filter source="Opaque/BlurSSAO">
            <defines>
                <define name="gs_nAxis" value="float2(0,1)" />
                <define name="gs_bCombine" value="1" />
            </defines>
        </filter>
        <!--filter source="Opaque/SSR" /-->
    </filters>
</pass>