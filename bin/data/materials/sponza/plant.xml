<?xml version="1.0"?>
<material>
    <shader>standard</shader>
    <maps pass="solid">
        <albedo file="$material.Albedo" />
        <normal file="$material.Normal" />
        <roughness file="$material.Roughness" />
        <metalness pattern="color" parameters="0" />
    </maps>
	<surface ghost="true" />
</material>
